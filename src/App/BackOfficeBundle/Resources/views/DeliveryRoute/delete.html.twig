{% extends "AppBackOfficeBundle::pagelayout.html.twig" %}

{% block content %}
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h3>Delete delivery zone "{{ entity.name }}"?</h3>
                {{ form_start(form) }}
                    <div>
                        {{ form_widget(form.submit) }}
                        <a href="../" class="btn">Cancel</a>
                    </div>
                {{ form_end(form) }} 
            </div>
        </div>
    </div>
{% endblock %}
